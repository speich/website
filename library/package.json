{
	"name": "speich.net",
	"description": "Dependencies for the website speich.net",
	"homepage": "https://www.speich.net",
	"repository": "github:speich/website",
	"version": "0.1.0",
	"dependencies": {
		"corejs-typeahead": "1.x",
		"dialog-confirm": "https://github.com/speich/DialogConfirm/",
		"dgrid": "https://github.com/SitePen/dgrid#v0.3.21",
		"dijit": "1.x",
		"dojo": "1.x",
		"dojox": "1.x",
		"photoswipe": "4.x",
		"put-selector": "^0.3.6",
		"tinyamd": "https://github.com/speich/tinyamd",
		"xstyle": "^0.3.3",
		"@google/markerclustererplus": "5.x"
	},
	"devDependencies": {
		"copyfiles": "2.x",
		"csso-cli": "3.x",
		"rimraf": "3.x",
		"uglifyjs-folder": "2.x"
	},
	"scripts": {
		"dialog-confirm:build": "cp node_modules/dialog-confirm/DialogConfirm.js speich.net/DialogConfirm/DialogConfirm.js && uglifyjs-folder speich.net/DialogConfirm -eo speich.net/DialogConfirm -x .js",
		"dgrid:del": "VERSION=$(node -p \"require('dgrid/package.json').version\") && rimraf dgrid/$VERSION",
		"dgrid:copy": "VERSION=$(node -p \"require('dgrid/package.json').version\") && copyfiles -u 2 -e **/test/** -e **/demos/** -e **/doc/** 'node_modules/dgrid/**/*' dgrid/$VERSION",
		"dgrid:js": "VERSION=$(node -p \"require('dgrid/package.json').version\") && uglifyjs-folder dgrid/$VERSION -eo dgrid/$VERSION -x .js",
		"dgrid:css": "VERSION=$(node -p \"require('dgrid/package.json').version\") && csso dgrid/$VERSION/css/dgrid.css -o dgrid/$VERSION/css/dgrid.css",
		"dgrid:build": "npm run dgrid:del && npm run dgrid:copy && npm run dgrid:js",
		"dojo:del": "VERSION=$(node -p \"require('dojo/package.json').version\") && rimraf dojo/$VERSION/dojo",
		"dojo:copy": "VERSION=$(node -p \"require('dojo/package.json').version\") && copyfiles -u 2 -e \"**/tests/**\" -e \"**/testsDOH/**\" \"node_modules/dojo/**/*\" dojo/$VERSION/dojo",
		"dojo:js": "VERSION=$(node -p \"require('dojo/package.json').version\") && uglifyjs-folder dojo/$VERSION/dojo -eo dojo/$VERSION/dojo -x .js",
		"dojo:build": "npm run dojo:del && npm run dojo:copy && npm run dojo:js",
		"dijit:del": "VERSION=$(node -p \"require('dijit/package.json').version\") && rimraf dojo/$VERSION/dijit",
		"dijit:copy": "VERSION=$(node -p \"require('dijit/package.json').version\") && copyfiles -u 2 -e \"**/tests/**\" -e \"**/tests/**\" \"node_modules/dijit/**/*\" dojo/$VERSION/dijit",
		"dijit:js": "VERSION=$(node -p \"require('dijit/package.json').version\") && uglifyjs-folder dojo/$VERSION/dijit -eo dojo/$VERSION/dijit -x .js",
		"dijit:build": "npm run dijit:del && npm run dijit:copy && npm run dijit:js",
		"dojox:del": "VERSION=$(node -p \"require('dojox/package.json').version\") && rimraf dojo/$VERSION/dojox",
		"dojox:copy": "VERSION=$(node -p \"require('dojox/package.json').version\") && copyfiles -u 2 -e \"**/tests/**\" -e \"**/tests/**\" \"node_modules/dojox/**/*\" dojo/$VERSION/dojox",
		"dojox:js": "VERSION=$(node -p \"require('dojox/package.json').version\") && uglifyjs-folder dojo/$VERSION/dojox -eo dojo/$VERSION/dojox -x .js",
		"dojox:build": "npm run dojox:del && npm run dojox:copy && npm run dojox:js",
		"markerclustererplus:del": "VERSION=$(node -p \"require('@google/markerclustererplus/package.json').version\") && rimraf markerclustererplus/$VERSION",
		"markerclustererplus:copy": "VERSION=$(node -p \"require('@google/markerclustererplus/package.json').version\") && copyfiles -u 3 \"node_modules/@google/markerclustererplus/dist/*.js\" \"node_modules/@google/markerclustererplus/images/*\" markerclustererplus/$VERSION",
		"markerclustererplus:js": "VERSION=$(node -p \"require('@google/markerclustererplus/package.json').version\") && uglifyjs-folder markerclustererplus/$VERSION -eo markerclustererplus/$VERSION -x .min.js",
		"markerclustererplus:build": "npm run markerclustererplus:del && npm run markerclustererplus:copy && npm run markerclustererplus:js",
		"photoswipe:del": "VERSION=$(node -p \"require('photoswipe/package.json').version\") && rimraf photoswipe/$VERSION",
		"photoswipe:copy": "VERSION=$(node -p \"require('photoswipe/package.json').version\") && copyfiles -u 3 \"node_modules/photoswipe/dist/**/*\" photoswipe/$VERSION",
		"photoswipe:css": "VERSION=$(node -p \"require('photoswipe/package.json').version\") && csso photoswipe/$VERSION/photoswipe.css -o photoswipe/$VERSION/photoswipe.min.css && csso photoswipe/$VERSION/default-skin/default-skin.css -o photoswipe/$VERSION/default-skin/default-skin.min.css",
		"photoswipe:build": "npm run photoswipe:del && npm run photoswipe:copy && npm run photoswipe:css",
		"put-selector:build": "VERSION=$(node -p \"require('put-selector/package.json').version\") && copyfiles -u 2 node_modules/put-selector/LICENSE node_modules/put-selector/package.json put-selector/$VERSION && terser node_modules/put-selector/put.js -mc -o put-selector/$VERSION/put.js",
		"xstyle:del": "VERSION=$(node -p \"require('xstyle/package.json').version\") && rimraf xstyle/$VERSION",
		"xstyle:copy": "VERSION=$(node -p \"require('xstyle/package.json').version\") && copyfiles -u 2 -e **/build/** 'node_modules/xstyle/**/*' xstyle/$VERSION",
		"xstyle:js": "VERSION=$(node -p \"require('xstyle/package.json').version\") && uglifyjs-folder xstyle/$VERSION -eo xstyle/$VERSION -x .js",
		"xstyle:build": "npm run xstyle:del && npm run xstyle:copy && npm run xstyle:js",
		"tinyamd:build": "cp node_modules/tinyamd/tinyamd.min.js tinyamd.min.js",
		"typeahead:del": "VERSION=$(node -p \"require('corejs-typeahead/package.json').version\") && rimraf typeahead/$VERSION",
		"typeahead:copy": "VERSION=$(node -p \"require('corejs-typeahead/package.json').version\") && copyfiles -u 2 \"node_modules/corejs-typeahead/dist/*\" typeahead/$VERSION",
		"typeahead:build": "npm run typeahead:del && npm run typeahead:copy",
		"cleanup": "rimraf node_modules",
		"build:keep": "npm run dialog-confirm:build && npm run dgrid:build && npm run dojo:build && npm run dijit:build && npm run dojox:build && npm run markerclustererplus:build && npm run photoswipe:build && npm run put-selector:build && npm run xstyle:build && npm run tinyamd:build && npm run typeahead:build"
	}
}
