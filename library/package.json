{
	"name": "speich.net",
	"description": "Dependencies for the website speich.net",
	"homepage": "https://www.speich.net",
	"repository": "github:speich/website",
	"version": "0.1.0",
	"dependencies": {
		"@googlemaps/markerclustererplus": "^1.2.10",
		"dgrid": "https://github.com/SitePen/dgrid#v0.3.21",
		"dialog-confirm": "https://github.com/speich/DialogConfirm/",
		"dijit": "1.17.3",
		"dojo": "1.17.3",
		"dojox": "1.17.3",
		"jquery": "3.7.1",
		"photoswipe": "5.4.4",
		"prismjs": "1.29.0",
		"put-selector": "^0.3.6",
		"tinyamd": "https://github.com/speich/tinyamd",
		"xstyle": "^0.3.3"
	},
	"devDependencies": {
		"copyfiles": "^2.4.1",
		"csso-cli": "^4.0.2",
		"rimraf": "^5.0.7",
		"uglifyjs-folder": "^3.3.0"
	},
	"scripts": {
		"dialog-confirm:build": "cp node_modules/dialog-confirm/DialogConfirm.js speich.net/DialogConfirm/DialogConfirm.js && uglifyjs-folder speich.net/DialogConfirm -eo speich.net/DialogConfirm -x .js",
			"dgrid:copy": "rimraf dgrid && copyfiles -u 2 -e **/test/** -e **/demos/** -e **/doc/** 'node_modules/dgrid/**/*' dgrid",
			"dgrid:js": "uglifyjs-folder dgrid -eo dgrid -x .js",
			"dgrid:css": "csso dgrid/css/dgrid.css -o dgridcss/dgrid.css",
		"dgrid:build": "npm run dgrid:copy && npm run dgrid:js",
			"dojo:copy": "rimraf dojo/dojo && copyfiles -u 2 -e \"**/tests/**\" -e \"**/testsDOH/**\" \"node_modules/dojo/**/*\" dojo/dojo",
			"dojo:js": "uglifyjs-folder dojo/dojo -eo dojo/dojo -x .js",
		"dojo:build": "npm run dojo:copy && npm run dojo:js",
			"dijit:copy": "rimraf dojo/dijit && copyfiles -u 2 -e \"**/tests/**\" -e \"**/tests/**\" \"node_modules/dijit/**/*\" dojo/dijit",
			"dijit:js": "uglifyjs-folder dojo/dijit -eo dojo/dijit -x .js",
		"dijit:build": "npm run dijit:copy && npm run dijit:js",
			"dojox:copy": "rimraf dojo/dojox && copyfiles -u 2 -e \"**/tests/**\" -e \"**/tests/**\" \"node_modules/dojox/**/*\" dojo/dojox",
			"dojox:js": "uglifyjs-folder dojo/dojox -eo dojo/dojox -x .js",
		"dojox:build": "npm run dojox:copy && npm run dojox:js",
		"jquery:build": "rimraf jquery && copyfiles -u 3 node_modules/jquery/dist/**/* jquery",
			"markerclustererplus:copy": "rimraf markerclustererplus && copyfiles -u 3 \"node_modules/@googlemaps/markerclustererplus/dist/*.*\" \"node_modules/@googlemaps/markerclustererplus/images/*\" markerclustererplus",
			"markerclustererplus:js": "uglifyjs-folder markerclustererplus -eo markerclustererplus -x .min.js",
		"markerclustererplus:build": "npm run markerclustererplus:copy && npm run markerclustererplus:js",
			"photoswipe:copy": "rimraf photoswipe && copyfiles -u 3 \"node_modules/photoswipe/dist/**/*\" photoswipe",
			"photoswipe:css": "csso photoswipe/photoswipe.css -o photoswipe/photoswipe.min.css",
		"photoswipe:build": "npm run photoswipe:copy && npm run photoswipe:css",
			"prismjs:copy": "rimraf prismjs && copyfiles -u 2 \"node_modules/prismjs/**/*\" prismjs",
			"prismjs:js": "terser prismjs/prism.js -o prismjs/prism.min.js",
			"prismjs:css": "csso prismjs/themes/prism.css -o prismjs/themes/prism.min.css",
		"prismjs:build": "npm run prismjs:copy && npm run prismjs:js && npm run prismjs:css",
		"put-selector:build": "rimraf put-selector && copyfiles -u 2 node_modules/put-selector/LICENSE node_modules/put-selector/package.json put-selector && terser node_modules/put-selector/put.js -mc -o put-selector/put.js",
			"xstyle:copy": "rimraf xstyle/$VERSION && copyfiles -u 2 -e **/build/** 'node_modules/xstyle/**/*' xstyle",
			"xstyle:js": "uglifyjs-folder xstyle -eo xstyle -x .js",
		"xstyle:build": "npm run xstyle:copy && npm run xstyle:js",
		"tinyamd:build": "cp node_modules/tinyamd/tinyamd.min.js tinyamd.min.js",
		"build": "npm run dialog-confirm:build & npm run dgrid:build & npm run dojo:build & npm run dijit:build & npm run dojox:build & npm run markerclustererplus:build & npm run jquery:build & npm run photoswipe:build & npm run prismjs:build & npm run put-selector:build & npm run xstyle:build & npm run tinyamd:build"
	}
}
