{
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "require": {
    "speich/websitetemplate": "dev-master",
    "ext-sqlite3": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-exif": "*",
    "ext-intl": "*",
    "ext-imagick": "*"
  },
  "autoload": {
    "psr-4": {
      "speich\\": "../scripts/php/class",
      "PhotoDb\\": "../photo/photodb/scripts/php",
      "WebsiteTemplate\\": "vendor/speich/websitetemplate/scripts/php"
    },
    "include-path": ["/layout"]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/speich/WebsiteTemplate"
    }
  ],
  "name": "speich.net/website",
  "description": "Dependencies for the website www.speich.net",
  "license": ["CC-BY-NC-SA-4.0"]
}
